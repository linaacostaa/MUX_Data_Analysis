function [I_V_min_raw, I_V_max_raw] = I_V_min_max(dataTable)
    % Convert the table to a numeric array
    dataArray = table2array(dataTable);
    % Select every 4th column (i.e. columns 4, 8, 12, ...)
    colIndices = 4:4:size(dataArray,2);
    if isempty(colIndices)
        error('No columns found matching the current data (multiples of 4).');
    end
    currentData = dataArray(:, colIndices);
    % Get the overall (absolute) min and max values
    I_V_min_raw = min(currentData(:));
    I_V_max_raw = max(currentData(:));
end